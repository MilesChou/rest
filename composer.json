{
    "name": "mileschou/rest",
    "description": "REST client.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "suggest": {
        "laminas/laminas-diactoros": "Require ^2 if you want use laminas as HTTP factory"
    },
    "require-dev": {
        "laminas/laminas-diactoros": "^2",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^7 | ^8",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\Rest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
